<!DOCTYPE html>
<html lang="fr">
	<head>
		<meta charset="UTF-8">
		<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
		<link rel="shortcut icon" type="image/webp" href="{{ asset('assets/logo/logo.webp') }}"/>
		<title>
			{% block title %}{% endblock %}
		</title>


		{% block stylesheets %}
			{{ encore_entry_link_tags('front_styles') }}
		{% endblock %}
	</head>

	<body>
		{% block header %}
			{% include 'front/shared/header.html.twig' %}
		{% endblock %}

		<div class="topContainer"> {% block toptext %}{% endblock %}
			{% block content %}
				{% include 'front/shared/topContent.html.twig' %}
			{% endblock %}
		</div>


		{% block pagecontent %}{% endblock %}

		{% block footer %}
			{% include 'front/shared/footer.html.twig' %}
		{% endblock %}
		{% block javascripts %}
			{{ encore_entry_script_tags('front_scripts') }}
		{% endblock %}
		<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js" integrity="sha384-HwwvtgBNo3bZJJLYd8oVXjrBZt8cqVSpeBNS5n7C8IVInixGAoxmnlMuBnhbgrkm" crossorigin="anonymous"></script>
	</body>

</html>
